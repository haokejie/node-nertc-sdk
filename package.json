{
  "name": "nertc-electron-sdk",
  "version": "4.2.5-rc-941",
  "description": "nertc-electron-sdk",
  "main": "js/nertc_sdk.js",
  "types": "types/nertc_sdk.d.ts",
  "dependencies": {
    "bindings": "^1.5.0",
    "download": "^8.0.0",
    "extract": "^1.0.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^8.1.0",
    "just-task": "^1.4.1",
    "lodash.isequal": "^4.5.0",
    "node-gyp": "^8.2.0",
    "node-pre-build": "^0.1.6",
    "shelljs": "^0.8.4",
    "tar": "^6.1.11",
    "yuv-buffer": "^1.0.0",
    "yuv-canvas": "^1.2.6"
  },
  "devDependencies": {
    "@types/node": "^12.12.22",
    "ink-docstrap": "^1.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "build:dts": "rimraf ./types && tsc -p config_dts.json",
    "prebuild:ts": "npm run build:dts",
    "build:ts": "rimraf ./js && tsc -p config_ts.json",
    "install": "node-pre-build install --fall-back-to-build",
    "build:win32:x64": "node-pre-build build -r electron -rv 9.4.4 11.4.5 12.0.11 -a x64 -p",
    "build:win32:ia32": "node-pre-build build -r electron -rv 9.4.4 11.4.5 12.0.11 -a ia32 -p",
    "build:darwin": "node-pre-build build -r electron -rv 9.4.4 11.4.5 12.0.11 -a x64 -p"
  },
  "node_pre_build": {
    "sdk-dir": "nertc_sdk",
    "name-sdk": "nertc",
    "name-addon": "nertc",
    "build-tool": "node-gyp",
    "binary-dir": "build/Release",
    "sdk-group": "NERtcSDK",
    "addon-group": "electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/surebeli/node-nertc-sdk.git#feat/3.9.0/screencapture"
  },
  "files": [
    "js",
    "types",
    "scripts",
    "just-task.js",
    "package.json"
  ],
  "keywords": [
    "nim",
    "macos",
    "node",
    "napi",
    "native",
    "windows",
    "pc",
    "nertc",
    "nrtc",
    "rtc",
    "screenshare",
    "electron"
  ],
  "author": "surebeli <surebeli@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/surebeli/node-nertc-sdk/issues"
  },
  "homepage": "https://github.com/surebeli/node-nertc-sdk#readme"
}
